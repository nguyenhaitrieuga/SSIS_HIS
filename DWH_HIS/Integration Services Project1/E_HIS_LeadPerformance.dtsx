    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{47E5E362-53CD-411E-A68D-44A81B0D705A}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select ca.Name CampaignName, &#xA;&#x9;case when lt.RefferURL like '%utm%' then 'utm' else 'non-utm' end IsUtm,&#xA;&#x9;IFNULL(s.Name,'Unknown') Source,&#xA;    count(distinct l.PhoneNumber) cnt_Lead,&#xA;    count(distinct case when c.CurrentLevel &gt; 1 then l.PhoneNumber else NULL end) cnt_Profile,&#xA;    count(distinct case when c.CurrentLevel &gt; 2 then l.PhoneNumber else NULL end) cnt_CheckIn,&#xA;    count(distinct case when c.CurrentLevel = 4 then l.PhoneNumber else NULL end) cnt_Customer,&#xA;    sum(cpc.Amount) Amount&#xA;from Lead l&#xA;LEFT JOIN Campaign ca on l.CampaignId = ca.CampaignId&#xA;LEFT JOIN Source s on l.SourceId = s.SourceId&#xA;LEFT JOIN LeadTag lt on l.LeadId = lt.LeadId&#xA;LEFT JOIN Contact c on l.ContactId = c.ContactId&#xA;LEFT JOIN &#xA;&#x9;(Select cp.ContactId, sum(Amount) Amount from ContactPayment cp&#xA;&#x9;WHERE PaymentDate &gt;= '&quot; + @[User::__Param__FirstDayOfMonth] + &quot;' and PaymentDate &lt; '&quot; + @[User::__Param__LastDayOfMonth] + &quot;'&#xA;&#x9;GROUP BY cp.ContactId) cpc on l.ContactId = cpc.ContactId&#xA;where l.CampaignId is not null &#xA;&#x9;and l.CampaignId &lt;&gt; 0&#xA;    and l.IsDeleted = 0&#xA;    and l.CreatedDate &gt;= '&quot; + @[User::__Param__FirstDayOfMonth] + &quot;' and l.CreatedDate &lt; '&quot; + @[User::__Param__LastDayOfMonth] + &quot;'&#xA;GROUP BY ca.Name , &#xA;&#x9;case when lt.RefferURL like '%utm%' then 'utm' else 'non-utm' end ,&#xA;&#x9;IFNULL(s.Name,'Unknown')    &quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQLCmd">
      <DTS:VariableValue
        DTS:DataType="8">select ca.Name CampaignName, 
	case when lt.RefferURL like '%utm%' then 'utm' else 'non-utm' end IsUtm,
	IFNULL(s.Name,'Unknown') Source,
    count(distinct l.PhoneNumber) cnt_Lead,
    count(distinct case when c.CurrentLevel &gt; 1 then l.PhoneNumber else NULL end) cnt_Profile,
    count(distinct case when c.CurrentLevel &gt; 2 then l.PhoneNumber else NULL end) cnt_CheckIn,
    count(distinct case when c.CurrentLevel = 4 then l.PhoneNumber else NULL end) cnt_Customer,
    sum(cpc.Amount) Amount
from Lead l
LEFT JOIN Campaign ca on l.CampaignId = ca.CampaignId
LEFT JOIN Source s on l.SourceId = s.SourceId
LEFT JOIN LeadTag lt on l.LeadId = lt.LeadId
LEFT JOIN Contact c on l.ContactId = c.ContactId
LEFT JOIN 
	(Select cp.ContactId, sum(Amount) Amount from ContactPayment cp
	WHERE PaymentDate &gt;= '2021-07-01' and PaymentDate &lt; '2021-07-16'
	GROUP BY cp.ContactId) cpc on l.ContactId = cpc.ContactId
where l.CampaignId is not null 
	and l.CampaignId &lt;&gt; 0
    and l.IsDeleted = 0
    and l.CreatedDate &gt;= '2021-07-01' and l.CreatedDate &lt; '2021-07-16'
GROUP BY ca.Name , 
	case when lt.RefferURL like '%utm%' then 'utm' else 'non-utm' end ,
	IFNULL(s.Name,'Unknown')    </DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{813CC0A6-79FD-4227-BABC-D2397FFA7856}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select ca.Name CampaignName, &#xA;&#x9;case when lt.RefferURL like '%utm%' then 'utm' else 'non-utm' end IsUtm,&#xA;&#x9;IFNULL(s.Name,'Unknown') Source,&#xA;    count(distinct l.PhoneNumber) cnt_Lead,&#xA;    -- count(distinct case when c.ContactId is null then LeadId else NULL end) cnt_Contact,&#xA;    count(distinct case when a.AppointmentId is not null then l.PhoneNumber else NULL end) cnt_Profile,&#xA;    count(distinct case when a.Status &gt;= 50 then l.PhoneNumber else NULL end) cnt_CheckIn,&#xA;    count(distinct case when cpa.AppointmentId is not null then l.PhoneNumber else NULL end) cnt_Customer,&#xA;    IFNULL(Sum(cpc.Amount),0) Amount &#xA;    from Lead l&#xA;LEFT JOIN Campaign ca on l.CampaignId = ca.CampaignId&#xA;LEFT JOIN Source s on l.SourceId = s.SourceId&#xA;LEFT JOIN LeadTag lt on l.LeadId = lt.LeadId&#xA;LEFT JOIN Contact c on l.ContactId = c.ContactId&#xA;LEFT JOIN Appointment a on l.LeadId = a.LeadId&#xA;LEFT JOIN &#xA;(Select cp.AppointmentId, sum(Amount) Amount from ContactPayment cp&#xA;WHERE PaymentDate &gt;= '&quot; + @[User::Month_From] + &quot;' and PaymentDate &lt; '&quot; + @[User::Month_To] + &quot;'&#xA;GROUP BY cp.AppointmentId) cpa&#xA;&#x9;on a.AppointmentId = cpa.AppointmentId&#xA;LEFT JOIN &#xA;(Select cp.ContactId, sum(Amount) Amount from ContactPayment cp&#xA;WHERE PaymentDate &gt;= '&quot; + @[User::Month_From] + &quot;' and PaymentDate &lt; '&quot; + @[User::Month_To] + &quot;'&#xA;GROUP BY cp.ContactId) cpc&#xA;&#x9;on l.ContactId = cpc.ContactId&#xA;where l.CampaignId is not null &#xA;&#x9;and l.CampaignId &lt;&gt; 0&#xA;    and l.IsDeleted = 0&#xA;    and l.CreatedDate &gt;= '&quot; + @[User::Month_From] + &quot;' and l.CreatedDate &lt; '&quot; + @[User::Month_To] + &quot;'&#xA;GROUP BY ca.Name, case when lt.RefferURL like '%utm%' then 'utm' else 'non-utm' end,&#xA;&#x9;IFNULL(s.Name,'Unknown')&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQLCmd_Backup">
      <DTS:VariableValue
        DTS:DataType="8">select ca.Name CampaignName, 
	case when lt.RefferURL like '%utm%' then 'utm' else 'non-utm' end IsUtm,
	IFNULL(s.Name,'Unknown') Source,
    count(distinct l.PhoneNumber) cnt_Lead,
    -- count(distinct case when c.ContactId is null then LeadId else NULL end) cnt_Contact,
    count(distinct case when a.AppointmentId is not null then l.PhoneNumber else NULL end) cnt_Profile,
    count(distinct case when a.Status &gt;= 50 then l.PhoneNumber else NULL end) cnt_CheckIn,
    count(distinct case when cpa.AppointmentId is not null then l.PhoneNumber else NULL end) cnt_Customer,
    IFNULL(Sum(cpc.Amount),0) Amount 
    from Lead l
LEFT JOIN Campaign ca on l.CampaignId = ca.CampaignId
LEFT JOIN Source s on l.SourceId = s.SourceId
LEFT JOIN LeadTag lt on l.LeadId = lt.LeadId
LEFT JOIN Contact c on l.ContactId = c.ContactId
LEFT JOIN Appointment a on l.LeadId = a.LeadId
LEFT JOIN 
(Select cp.AppointmentId, sum(Amount) Amount from ContactPayment cp
WHERE PaymentDate &gt;= '2020-12-01' and PaymentDate &lt; '2020-01-01'
GROUP BY cp.AppointmentId) cpa
	on a.AppointmentId = cpa.AppointmentId
LEFT JOIN 
(Select cp.ContactId, sum(Amount) Amount from ContactPayment cp
WHERE PaymentDate &gt;= '2020-12-01' and PaymentDate &lt; '2020-01-01'
GROUP BY cp.ContactId) cpc
	on l.ContactId = cpc.ContactId
where l.CampaignId is not null 
	and l.CampaignId &lt;&gt; 0
    and l.IsDeleted = 0
    and l.CreatedDate &gt;= '2020-12-01' and l.CreatedDate &lt; '2020-01-01'
GROUP BY ca.Name, case when lt.RefferURL like '%utm%' then 'utm' else 'non-utm' end,
	IFNULL(s.Name,'Unknown')</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
